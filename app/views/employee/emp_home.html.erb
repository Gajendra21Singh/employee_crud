<h1 align='center'>Employee Records</h1>
<table align='center' , border = '1'>
  <tr>
    <td>Name</td>
    <td>Email Id</td>
    <td>Mobile No</td>
    <td>Salary</td>
    <td>Address</td>
  </tr>
  <%= @employee.each do |emp| %>
  <tr>
    <td><%= emp[:emp_name] %></td>
    <td><%= emp[:email_id] %></td>
    <td><%= emp[:mobile_no] %></td>
    <td><%= emp[:salary] %></td>
    <td><%= emp[:address] %></td>
    <td><%= %></td>
    <td><%= %></td>
    <td><%= %></td>
  </tr>
  <% end %>
</table>
